{
  "name": "concept_streetwear",
  "version": "1.0.0",
  "description": "Streetwear Landing Page",
  "main": "webpack.config.js",
  "scripts": {
    "sass": "node-sass ./src/scss/main.scss ./dist/css/style.css -w",
    "compile:sass": "node-sass scss/main.scss dist/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" dist/css/style.comp.css -o dist/css/style.prefix.css",
    "compress:css": "node-sass dist/css/style.prefix.css dist/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "start": "webpack-dev-server --mode development --open"
  },
  "repository": {
    "type": "git",
    "url": "concept_streetwear"
  },
  "keywords": [
    "concept",
    "streetwear",
    "aaron",
    "asuncion",
    "aaronpasuncion",
    "aaronasuncion",
    "front",
    "end",
    "development",
    "html",
    "html5",
    "css",
    "css3",
    "javascript",
    "gsap",
    "sass",
    "scss",
    "vanilla",
    "design",
    "fashion",
    "street",
    "culture"
  ],
  "author": "Aaron Asuncion",
  "license": "ISC",
  "dependencies": {
    "node-sass": "^4.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "autoprefixer": "^9.8.5",
    "babel-loader": "^8.3.0",
    "concat": "^1.0.3",
    "core-js": "^3.6.5",
    "install": "^0.13.0",
    "npm": "^6.14.6",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "regenerator-runtime": "^0.13.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.11.0"
  }
}
